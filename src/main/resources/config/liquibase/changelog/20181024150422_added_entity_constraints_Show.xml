<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Show.
    -->
    <changeSet id="20181024150422-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="screen_id"
                                 baseTableName="show"
                                 constraintName="fk_show_screen_id"
                                 referencedColumnNames="id"
                                 referencedTableName="screen"/>

        <addForeignKeyConstraint baseColumnNames="movie_id"
                                 baseTableName="show"
                                 constraintName="fk_show_movie_id"
                                 referencedColumnNames="id"
                                 referencedTableName="movie"/>

    </changeSet>
</databaseChangeLog>
